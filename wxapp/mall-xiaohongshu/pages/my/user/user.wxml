<view class="page">
    <view class="userInfo">
        <view class="userinfo">
            <view class="head-img">
                <open-data class="thumb"type="userAvatarUrl"></open-data>
                <open-data class="name" lang="zh_CN" type="userNickName"></open-data>
            </view>
            <view class="userMessage">
                <view class="messageBox">
                    <view class="focus">
                        <span class="focusNum">0</span>
                        <span>关注</span>
                    </view>
                    <view class="border">|</view>
                    <view class="fans">
                        <span class="fansNum">1</span>
                        <span>粉丝</span>
                    </view>
                    <view class="border">|</view>
                    <view class="praise-collect">
                        <span class="colllectNum">0</span>
                        <span>赞与收藏</span>
                    </view>
                </view>
            </view>
        </view>
        <view class="grade">
            <image class="grade-icon" src="/images/grade-img.png"></image>
            <span>等级：薯北鼻</span>
        </view>
        <view class="userDesc">
            <span>还没有简介</span>
        </view>
    </view>
</view>
