<view class="weui-cells">
    <navigator class="weui-cell weui-cell_access head" url="/pages/home/auth/auth" hover-class="weui-cell_active">
        <view class="weui-cell__hd">
            <open-data class="thumb" type="userAvatarUrl"></open-data>
        </view>
        <view class="weui-cell__bd">
            <view class="name">{{userInfo.nickName}}</view>
            <view class="stu_id">{{stu_id}}</view>
        </view>
        <view class="weui-cell__ft  weui-cell__ft_in-access"></view>
    </navigator>
</view>
<view class="weui-cells" wx:if="{{userInfo.isBindEdu}}">
    <navigator class="weui-cell weui-cell_access" hover-class="weui-cell_active" url="/pages/score/score">
        <view class="weui-cell__hd">
            <image src="/images/icon_score.png"/>
        </view>
        <view class="weui-cell__bd">90</view>
        <view class="weui-cell__ft  weui-cell__ft_in-access"></view>
    </navigator>
</view>
