.page{
    height: 100vh;
    background: #F4F8FB;
}
.weui-cell.head {
    padding: 8px 14px;
}
.weui-cell.head image {
    width: 134rpx;
    height: 134rpx;
    margin-right: 16px;
    margin-left: 0;
}
.name {
    margin-top: 2rpx;       
}
.stu_id {
    font-size: 30rpx;
    margin-top: 8rpx;
    color: #999;
}
  
.weui-cell__hd {
    font-size: 0;
}
.weui-cell__hd image {
    width: 100rpx;
    height: 100rpx;
    margin-right: 18px;
    margin-left: 5px;
    vertical-align: middle;
}
.userInfo{
    height: 500rpx;
}
.thumb{
    width: 100px;
    height: 100px;
    display: block;
}
/* .name{
    display: block;
} */
.weui-cell__ft_in-access {
    padding-right:13px;
    position:relative;
}
    


